{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "installCommand": "cd ai-resume-frontend && npm install",
  "buildCommand": "cd ai-resume-frontend && npm run build",
  "outputDirectory": "ai-resume-frontend/dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
